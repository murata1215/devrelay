{
  "name": "devrelay",
  "version": "0.1.0",
  "private": true,
  "description": "Remote AI CLI development hub - Control Claude Code, Gemini CLI, and more from anywhere",
  "pnpm": {
    "onlyBuiltDependencies": ["electron", "esbuild", "@prisma/client", "@prisma/engines", "prisma"]
    ,"overrides": {
      "readable-stream": "2.3.8"
    }
  },
  "scripts": {
    "dev:server": "pnpm --filter @devrelay/server dev",
    "dev:agent": "pnpm --filter @devrelay/agent-linux dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean"
  },
  "keywords": [
    "ai",
    "claude-code",
    "remote-development",
    "cli",
    "discord",
    "telegram"
  ],
  "author": "DevRelay",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
